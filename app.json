{
  "expo": {
    "name": "Eve-Net",
    "slug": "Eve-Net",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon512.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com._yourValue_.eve",
      "versionCode": 1,
      "config": {
        "googleMaps": { "apiKey": "__Your_API_Key__" }   
     }
    }
  }
}
